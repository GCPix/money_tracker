<h1><%=@merchant.name%></h1>



<form method="get" action = "<%=@merchant.id%>/transactions">

    <input class = "button" type="submit" value="Show Transactions for Merchant"/>
</form>

<form method="get" action = "/merchants/<%=@merchant.id%>/edit">

    <input class = "button" type="submit" value="Edit Merchant Details"/>
</form>

